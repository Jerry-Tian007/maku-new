<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.gpt.mapper.QAToneStyleMapper">

    <resultMap type="QAToneStyle" id="QAToneStyleResult">
        <result property="id" column="id"/>
        <result property="model" column="model"/>
        <result property="tone" column="tone"/>
        <result property="style" column="style"/>
        <result property="temperature" column="temperature"/>
        <result property="createTime" column="create_time"/>
        <result property="createBy" column="create_by"/>
    </resultMap>

    <sql id="selectQAToneStyleVo">
        select id, model, tone, style, temperature, create_time, create_by
        from q_a_tone_style
    </sql>

    <select id="selectQAToneStyleList" parameterType="QAToneStyle" resultMap="QAToneStyleResult">
        <include refid="selectQAToneStyleVo"/>
        <where>
            <if test="model != null  and model != ''">and model = #{model}</if>
            <if test="tone != null  and tone != ''">and tone = #{tone}</if>
            <if test="style != null  and style != ''">and `style` = #{style}</if>
            <if test="temperature != null  and temperature != ''">and temperature = #{temperature}</if>
        </where>
    </select>

    <select id="selectQAToneStyleById" parameterType="Long" resultMap="QAToneStyleResult">
        <include refid="selectQAToneStyleVo"/>
        where id = #{id}
    </select>

    <insert id="insertQAToneStyle" parameterType="QAToneStyle" useGeneratedKeys="true" keyProperty="id">
        insert into q_a_tone_style
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="model != null">model,</if>
            <if test="tone != null">tone,</if>
            <if test="style != null">`style`,</if>
            <if test="temperature != null">temperature,</if>
            <if test="createTime != null">create_time,</if>
            <if test="createBy != null">create_by,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="model != null">#{model},</if>
            <if test="tone != null">#{tone},</if>
            <if test="style != null">#{style},</if>
            <if test="temperature != null">#{temperature},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="createBy != null">#{createBy},</if>
        </trim>
    </insert>

    <update id="updateQAToneStyle" parameterType="QAToneStyle">
        update q_a_tone_style
        <trim prefix="SET" suffixOverrides=",">
            <if test="model != null">model = #{model},</if>
            <if test="tone != null">tone = #{tone},</if>
            <if test="style != null">`style` = #{style},</if>
            <if test="temperature != null">temperature = #{temperature},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteQAToneStyleById" parameterType="Long">
        delete
        from q_a_tone_style
        where id = #{id}
    </delete>

    <delete id="deleteQAToneStyleByIds" parameterType="String">
        delete from q_a_tone_style where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <select id="getQAToneStyleDetail" parameterType="QAToneStyle" resultMap="QAToneStyleResult">
        <include refid="selectQAToneStyleVo"/>
        <where>
            <if test="model != null  and model != ''">and model = #{model}</if>
            <if test="tone != null  and tone != ''">and tone = #{tone}</if>
            <if test="style != null  and style != ''">and `style` = #{style}</if>
        </where>
        limit 1
    </select>
</mapper>